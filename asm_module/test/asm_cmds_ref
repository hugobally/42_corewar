.name "toto"
.comment "toto"

## live
l1:

live %TEST_DIR
live %:l1

## load

l2:

ld %TEST_DIR, TEST_REG
ld %:l1, TEST_REG

ld TEST_IND, TEST_REG
ld :l1, TEST_REG

## store

l3:

st TEST_REG, TEST_REG

st TEST_REG, TEST_IND
st TEST_REG, :l1

## add / sub

l4:

add TEST_REG, TEST_REG, TEST_REG
sub TEST_REG, TEST_REG, TEST_REG

l5:
